#################################################################
#    Rules
#################################################################

:s rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:c rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:cc rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:e rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:d rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

[ <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> "true"^^xsd:boolean ;
   rdfs:comment "Classifies episodes into long ones if duration is greater than 60 minutes." ;
   rdfs:label "longEpisodeDef" ;
   rdf:type <http://www.w3.org/2003/11/swrl#Imp> ;
   <http://www.w3.org/2003/11/swrl#body> [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                           rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#DatavaluedPropertyAtom> ;
                                                       <http://www.w3.org/2003/11/swrl#propertyPredicate> :has_duration ;
                                                       <http://www.w3.org/2003/11/swrl#argument1> :e ;
                                                       <http://www.w3.org/2003/11/swrl#argument2> :d
                                                     ] ;
                                           rdf:rest [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                                      rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#BuiltinAtom> ;
                                                                  <http://www.w3.org/2003/11/swrl#builtin> <http://www.w3.org/2003/11/swrlb#greaterThan> ;
                                                                  <http://www.w3.org/2003/11/swrl#arguments> [ rdf:type rdf:List ;
                                                                                                               rdf:first :d ;
                                                                                                               rdf:rest [ rdf:type rdf:List ;
                                                                                                                          rdf:first 60 ;
                                                                                                                          rdf:rest rdf:nil
                                                                                                                        ]
                                                                                                             ]
                                                                ] ;
                                                      rdf:rest [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                                                 rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#ClassAtom> ;
                                                                             <http://www.w3.org/2003/11/swrl#classPredicate> <http://www.semanticweb.org/mongay_mulet/ontologies/game_of_thrones_ontology-1.0.0#Episode> ;
                                                                             <http://www.w3.org/2003/11/swrl#argument1> :e
                                                                           ] ;
                                                                 rdf:rest rdf:nil
                                                               ]
                                                    ]
                                         ] ;
   <http://www.w3.org/2003/11/swrl#head> [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                           rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#ClassAtom> ;
                                                       <http://www.w3.org/2003/11/swrl#classPredicate> <http://www.semanticweb.org/mongay_mulet/ontologies/game_of_thrones_ontology-1.0.0#LongEpisode> ;
                                                       <http://www.w3.org/2003/11/swrl#argument1> :e
                                                     ] ;
                                           rdf:rest rdf:nil
